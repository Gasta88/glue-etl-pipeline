                        ]
                    },
                    "timestamp": {
                        "type": "integer",
                        "required": true
                    },
                    "input": {
                        "type": "dict",
                        "required": true,
                        "schema": {
                            "transcript": {
                                "type": "string",
                                "required": true
                            }
                        }
                    },
                    "output": {
                        "type": "dict",
                        "required": true,
                        "schema": {
                            "headline": {
                                "type": "list",
                                "required": true,
